<div class="displayWindow">
<h1>Results : </h1>
<% if @questions != nil %>
    <h4>Number of Results: <%= @questions.length %> </h4>
    <% number = 1 %>
    <% @questions.each do |question| %>
        <div class="card">
            <%= number %>.
            <%= question.title %>
            
            <%= link_to 'SHOW', question_path(question.id) %>
            <% number+=1 %>
        <div>
    <% end %>
<%else%>
    <h4>No such posts found</h4>
<% end %>
</div>